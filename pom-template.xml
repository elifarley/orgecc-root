<?xml version="1.0" encoding="UTF-8"?>
<project xmlns="http://maven.apache.org/POM/4.0.0"
  xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
  xsi:schemaLocation="http://maven.apache.org/POM/4.0.0 http://maven.apache.org/maven-v4_0_0.xsd">

  <modelVersion>4.0.0</modelVersion>

  <parent>
    <groupId>com.orgecc</groupId>
    <artifactId>orgecc-root</artifactId>
    <version>default-SNAPSHOT</version>
  </parent>

  <artifactId><$NAME>-root</artifactId>
  <version>default-SNAPSHOT</version>
  <packaging>pom</packaging>
  <name><$PROJECT></name>
  <description><$DESC></description>
  <inceptionYear><$YEAR></inceptionYear>

  <properties>
  </properties>

  <modules>
    <module>ejb</module>
    <module>war</module>
    <module>ear</module>
  </modules>

  <organization>
    <name><$NAME></name>
    <url>http://www.<$DOMAIN></url>
  </organization>

  <developers>
    <developer>
      <id><$ID></id>
      <name><$NOME></name>
      <email><$EMAIL></email>
      <roles>
        <role><$PAPEL></role>
      </roles>
      <organization><$EMPRESA></organization>
      <timezone>-3</timezone>
    </developer>
  </developers>
  <contributors>
    <contributor>
      <name><$NOME></name>
      <email><$EMAIL></email>
    </contributor>
  </contributors>

  <build>

    <defaultGoal>install</defaultGoal>
    <finalName><$PROJETO></finalName>

    <pluginManagement>
      <plugins>

        <plugin>
          <groupId><$GRUPO-DO-PLUGIN></groupId>
          <artifactId><$ARTEFATO-DO-PLUGIN></artifactId>
          <configuration>
          </configuration>
        </plugin>

      </plugins>
    </pluginManagement>

    <plugins>

      <plugin>
        <groupId><$GRUPO-DO-PLUGIN></groupId>
        <artifactId><$ARTEFATO-DO-PLUGIN></artifactId>
        <configuration>
        </configuration>
      </plugin>

    </plugins>

  </build>

  <dependencyManagement>
    <dependencies>

      <dependency>
        <groupId>com.orgecc</groupId>
        <artifactId>orgecc-dep-pom</artifactId>
        <version>default-SNAPSHOT</version>
        <type>pom</type>
        <scope>import</scope>
      </dependency>

      <dependency>
        <groupId><$GRUPO-DA-DEP></groupId>
        <artifactId><$ARTEFATO-DA-DEP></artifactId>
        <version>1.0-SNAPSHOT</version>
      </dependency>

    </dependencies>
  </dependencyManagement>

  <dependencies>

    <dependency>
      <groupId>com.orgecc</groupId>
      <artifactId>orgecc-root-info</artifactId>
    </dependency>

    <dependency>
      <groupId><$GRUPO-DA-DEP></groupId>
      <artifactId><$ARTEFATO-DA-DEP></artifactId>
      <version>1.0-SNAPSHOT</version>
    </dependency>

  </dependencies>

</project>